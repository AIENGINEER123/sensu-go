#!/bin/sh
<%= (common_files || "").split(",").inject("") { |res, f| res + File.read(File.join(prefix, f)) + "\n" } %>

case "$1" in
    remove|purge|upgrade|disappear|failed-upgrade|abort-install|abort-upgrade)
	;;

    *)
	echo "postrm called with unknown argument \`$1'" >&2
	exit 1
	;;
esac

exit 0
